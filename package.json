    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.5.2",
    "mint-ui": "^2.2.13",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
